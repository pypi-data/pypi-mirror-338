<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../utilities/dd_support.xsd"/>
	<!-- For this initial version, the coil substructure and its children are simply duplicated from the TF IDS. If no changes to the coil substructure are needed (w.r.t. the TF coil IDS), we should be moving these types to utilities and make them common to both IDSs -->
	<xs:complexType name="coil_conductor_elements">
		<xs:annotation>
			<xs:documentation>Elements descibring the conductor contour</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="names">
				<xs:annotation>
					<xs:documentation>Name or description of every element</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="types">
				<xs:annotation>
					<xs:documentation>Type of every element: 1: line segment, its ends are given by the start and end points; index = 2: arc of a circle; index = 3: full circle</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>../names</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="start_points" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of the start point of every element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="intermediate_points" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of an intermediate point along the arc of circle, for every element, providing the orientation of the element (must define with the corresponding start point an aperture angle strictly inferior to PI). Meaningful only if type/index = 2, fill with default/empty value otherwise</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="end_points" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of the end point of every element. Meaningful only if type/index = 1 or 2, fill with default/empty value otherwise</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="centres" type="rzphi1d_static">
				<xs:annotation>
					<xs:documentation>Position of the centre of the arc of a circle of every element (meaningful only if type/index = 2 or 3, fill with default/empty value otherwise)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="coil_conductor">
		<xs:annotation>
			<xs:documentation>Description of a conductor</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="elements" type="coil_conductor_elements">
				<xs:annotation>
					<xs:documentation>Set of geometrical elements (line segments and/or arcs of a circle) describing the contour of the conductor centre</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cross_section" type="delta_rzphi1d_static">
				<xs:annotation>
					<xs:documentation>The cross-section perpendicular to the conductor contour is described by a series of contour points, given by their relative position with respect to the start point of the first element. This cross-section is assumed constant for all elements.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="resistance">
				<xs:annotation>
					<xs:documentation>conductor resistance</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>Ohm</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="current" type="signal_flt_1d">
				<xs:annotation>
					<xs:documentation>Current in the conductor (positive when it flows from the first to the last element)</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="voltage" type="signal_flt_1d">
				<xs:annotation>
					<xs:documentation>Voltage on the conductor terminals</xs:documentation>
					<xs:appinfo>
						<units>V</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="coil">
		<xs:annotation>
			<xs:documentation>Description of a given coil</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>Name of the coil</xs:documentation>
					<xs:appinfo>
						<type>static</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="identifier">
				<xs:annotation>
					<xs:documentation>Alphanumeric identifier of coil</xs:documentation>
					<xs:appinfo>
						<type>static</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="conductor" type="coil_conductor" maxOccurs="20">
				<xs:annotation>
					<xs:documentation>Set of conductors inside the coil. The structure can be used with size 1 for a simplified description as a single conductor. A conductor is composed of several elements, serially connected, i.e. transporting the same current.</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="turns">
				<xs:annotation>
					<xs:documentation>Number of total turns in the coil. May be a fraction when describing the coil connections.</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="resistance">
				<xs:annotation>
					<xs:documentation>Coil resistance</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>Ohm</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="current" type="signal_flt_1d">
				<xs:annotation>
					<xs:documentation>Current in one turn of the coil (to be multiplied by the number of turns to calculate the magnetic field generated). Sign convention : a positive current generates a positive radial magnetic field</xs:documentation>
					<xs:appinfo>
						<units>A</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="voltage" type="signal_flt_1d">
				<xs:annotation>
					<xs:documentation>Voltage on the coil terminals. Sign convention : a positive power supply voltage (and power supply current) generates a positive radial magnetic field</xs:documentation>
					<xs:appinfo>
						<units>V</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="coils_non_axisymmetric">
		<xs:annotation>
			<xs:documentation>Non axisymmetric active coils system (e.g. ELM control coils, error field correction coils, ...)</xs:documentation>
			<xs:appinfo>
				<lifecycle_status>alpha</lifecycle_status>
				<lifecycle_version>3.19.1</lifecycle_version>
				<lifecycle_last_change>3.33.0</lifecycle_last_change>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ids_properties"/>
				<xs:element name="coil" type="coil" maxOccurs="32">
					<xs:annotation>
						<xs:documentation>Set of coils</xs:documentation>
						<xs:appinfo>
							<type>static</type>
							<coordinate1>1...N</coordinate1>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="latency">
					<xs:annotation>
						<xs:documentation>Upper bound of the delay between input command received from the RT network and actuator starting to react. Applies globally to the system described by this IDS unless specific latencies (e.g. channel-specific or antenna-specific) are provided at a deeper level in the IDS structure.</xs:documentation>
						<xs:appinfo>
							<type>static</type>
							<units>s</units>
							<introduced_after_version>3.32.1</introduced_after_version>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:group ref="FLT_0D"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="code"/>
				<xs:element ref="time"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
