				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="gyrokinetics_collisions">
		<xs:annotation>
			<xs:documentation>Collisions related quantities</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="collisionality_norm">
