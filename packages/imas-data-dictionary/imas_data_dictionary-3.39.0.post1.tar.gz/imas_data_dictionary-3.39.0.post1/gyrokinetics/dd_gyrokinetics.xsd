	<xs:complexType name="gyrokinetics_model">
		<xs:annotation>
			<xs:documentation>Description of the GK model assumptions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="include_centrifugal_effects">
				<xs:annotation>
					<xs:documentation>Flag = 1 if centrifugal effects are retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="include_a_field_parallel">
				<xs:annotation>
					<xs:documentation>Flag = 1 if fluctuations of the parallel vector potential are retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="include_b_field_parallel">
				<xs:annotation>
					<xs:documentation>Flag = 1 if fluctuations of the parallel magnetic field are retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="include_full_curvature_drift">
				<xs:annotation>
					<xs:documentation>Flag = 1 if all contributions to the curvature drift are included (including beta_prime), 0 otherwise. Neglecting the beta_prime contribution (Flag=0) is only recommended together with the neglect of parallel magnetic field fluctuations</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_pitch_only">
				<xs:annotation>
					<xs:documentation>Flag = 1 if only pitch-angle scattering is retained, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_momentum_conservation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the collision operator conserves momentum, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_energy_conservation">
				<xs:annotation>
					<xs:documentation>Flag = 1 if the collision operator conserves energy, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="collisions_finite_larmor_radius">
				<xs:annotation>
					<xs:documentation>Flag = 1 if finite larmor radius effects are retained in the collision operator, 0 otherwise</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="non_linear_run">
				<xs:annotation>
					<xs:documentation>Flag = 1 if this is a non-linear run, 0 for a linear run</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_interval_norm">
				<xs:annotation>
					<xs:documentation>Normalised time interval used to average fluxes in non-linear runs</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>1...2</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
