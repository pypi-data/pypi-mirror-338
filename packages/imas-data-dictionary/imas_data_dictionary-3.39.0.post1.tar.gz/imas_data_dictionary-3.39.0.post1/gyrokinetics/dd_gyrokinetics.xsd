					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../../../poloidal_angle</coordinate1>
						<coordinate2>../../../time_norm</coordinate2>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="CPX_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="v_perpendicular_square_energy">
				<xs:annotation>
					<xs:documentation>Normalised moment (integral over 0.5 * m * v_perp^2 * v^2)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../../../poloidal_angle</coordinate1>
