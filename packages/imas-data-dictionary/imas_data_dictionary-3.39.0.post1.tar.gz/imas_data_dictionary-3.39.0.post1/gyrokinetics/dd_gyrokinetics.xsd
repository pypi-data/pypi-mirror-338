					<xs:documentation>Normalised perpendicular temperature</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../../../poloidal_angle</coordinate1>
						<coordinate2>../../../time_norm</coordinate2>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="CPX_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="heat_flux_parallel">
				<xs:annotation>
					<xs:documentation>Normalised parallel heat flux (integral of 0.5 * m * v_par * v^2)</xs:documentation>
