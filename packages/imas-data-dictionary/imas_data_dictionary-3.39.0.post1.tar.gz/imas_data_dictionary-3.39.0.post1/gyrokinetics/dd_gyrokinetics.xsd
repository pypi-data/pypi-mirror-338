					<xs:annotation>
						<xs:documentation>Flux surface characteristics</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="model" type="gyrokinetics_model">
					<xs:annotation>
						<xs:documentation>Assumptions of the GK calculations</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="species_all" type="gyrokinetics_input_species_global">
					<xs:annotation>
						<xs:documentation>Physical quantities common to all species</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="species" type="gyrokinetics_species" maxOccurs="10">
					<xs:annotation>
						<xs:documentation>Set of species (including electrons) used in the calculation and related quantities</xs:documentation>
						<xs:appinfo>
							<coordinate1>1...N</coordinate1>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="collisions" type="gyrokinetics_collisions">
