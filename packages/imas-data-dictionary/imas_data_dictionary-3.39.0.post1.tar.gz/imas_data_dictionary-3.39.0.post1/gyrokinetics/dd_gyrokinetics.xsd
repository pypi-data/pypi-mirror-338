				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="growth_rate_norm">
				<xs:annotation>
					<xs:documentation>Growth rate</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
