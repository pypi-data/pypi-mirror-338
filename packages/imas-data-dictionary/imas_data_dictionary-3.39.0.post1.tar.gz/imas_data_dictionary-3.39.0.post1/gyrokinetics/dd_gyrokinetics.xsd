				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="binormal_component_norm">
				<xs:annotation>
					<xs:documentation>Normalised binormal component of the wavevector</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
					</xs:appinfo>
