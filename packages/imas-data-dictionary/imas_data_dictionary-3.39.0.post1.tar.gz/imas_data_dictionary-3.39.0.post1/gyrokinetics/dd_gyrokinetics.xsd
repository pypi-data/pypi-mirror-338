				</xs:annotation>
				<xs:complexType>
					<xs:group ref="CPX_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time_norm">
				<xs:annotation>
					<xs:documentation>Normalised time of the gyrokinetic simulation</xs:documentation>
