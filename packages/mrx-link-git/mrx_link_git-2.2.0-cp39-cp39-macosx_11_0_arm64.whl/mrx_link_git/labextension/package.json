{
  "name": "mrx_link_git",
  "version": "2.2.0",
  "description": "A Linkâ„¢ extension for version control using Git.",
  "private": true,
  "author": {
    "name": "makinarocks",
    "email": "devops@makinarocks.ai"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "license": "BSD-3-Clause",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "git"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension",
    "build:labextension": "jupyter labextension build --development True .",
    "build:labextension:prod": "jupyter labextension build --development False .",
    "build:lib": "genversion --es6 --semi src/version.ts && tsc",
    "build:prod": "npm run build:lib && npm run build:labextension:prod",
    "clean": "npm run clean:lib",
    "clean:all": "npm run clean:lib && npm run clean:labextension",
    "clean:labextension": "rimraf mrx_link_git/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:more": "npm clean && rimraf build dist MANIFEST",
    "clean:slate": "npm clean:more && npm clean:labextension && rimraf node_modules",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/**/*.{json,}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
