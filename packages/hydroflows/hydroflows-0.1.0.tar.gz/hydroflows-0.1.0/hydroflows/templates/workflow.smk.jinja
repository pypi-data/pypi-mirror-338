{% from "rule.smk.jinja" import add_rule %}
# This file was generated by hydroflows version {{ version }}

configfile: "{{ configfile }}"

{% for key in wildcards %}
{{ key|upper }}=config["{{ key|upper }}"]
{% endfor %}

rule all:
    input:
        {% for rule in rules %}
        {% if rule.rule_all_input %}
            {{ rule.rule_all_input }},
        {% endif %}
        {% endfor %}

{% for rule in rules %}
{% with rule=rule %}
{{ add_rule(rule, dryrun) }}
{% endwith %}
{% endfor %}
