{
    "name": "maap_che_sidebar_visibility_jupyter_extension",
    "version": "1.1.3",
    "description": "A Jupyter extension that provides users the ability to hide the Eclipse Che sidebar in the MAAP ADE.",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/MAAP-Project/che-sidebar-visibility-jupyter-extension",
    "bugs": {
        "url": "https://github.com/MAAP-Project/che-sidebar-visibility-jupyter-extension/issues"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Anil Natha",
        "email": "anil.natha@jpl.nasa.gov"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/MAAP-Project/che-sidebar-visibility-jupyter-extension.git"
    },
    "scripts": {
        "build": "jlpm build:lib && jlpm build:labextension:dev",
        "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "jlpm clean:lib",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:labextension": "rimraf maap_che_sidebar_visibility_jupyter_extension/labextension maap_che_sidebar_visibility_jupyter_extension/_version.py",
        "clean:all": "jlpm clean:lib && jlpm clean:labextension",
        "install:extension": "jlpm build",
        "start": "jupyter lab --config ui-tests/jupyter_server_test_config.py",
        "test": "jest --coverage",
        "test:ui": "jlpm playwright test --ui",
        "watch": "run-p watch:src watch:labextension",
        "watch:src": "tsc -w",
        "watch:labextension": "jupyter labextension watch ."
    },
    "dependencies": {
        "@jupyterlab/application": "^4.0.0",
        "@jupyterlab/apputils": "^4.0.0",
        "@jupyterlab/coreutils": "^6.1.8",
        "@jupyterlab/rendermime-interfaces": "^3.9.8",
        "@jupyterlab/services": "^7.1.8",
        "@lumino/widgets": "^2.3.2",
        "@playwright/test": "^1.44.0"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.26.0",
        "@jupyterlab/builder": "^4.0.0",
        "@jupyterlab/testutils": "^4.0.0",
        "@types/jest": "^29.5.13",
        "jest": "^29.7.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "typescript": "~5.4.2"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "maap_che_sidebar_visibility_jupyter_extension/labextension"
    }
}
