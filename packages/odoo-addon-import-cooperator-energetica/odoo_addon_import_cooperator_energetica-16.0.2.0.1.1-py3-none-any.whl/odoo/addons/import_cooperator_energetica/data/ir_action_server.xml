<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="map_data_action" model="ir.actions.server">
            <field name="name">Map data</field>
            <field name="model_id" ref="model_import_cooperator" />
            <field name="state">code</field>
            <field name="code">action = model.create_subscription_request_or_partner()</field>
            <field name="website_published">False</field>
        </record>
        <record id="validate_subscription_request_action" model="ir.actions.server">
            <field name="name">Validate subscription request</field>
            <field name="model_id" ref="model_import_cooperator" />
            <field name="state">code</field>
            <field name="code">action = model.validate()</field>
            <field name="website_published">False</field>
        </record>
    </data>
</odoo>
